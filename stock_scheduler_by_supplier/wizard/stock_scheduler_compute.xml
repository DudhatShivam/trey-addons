<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_procurement_compute_wizard" model="ir.ui.view">
        <field name="model">stock.scheduler.compute</field>
        <field name="inherit_id" ref="stock.view_procurement_compute_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <group string="Supplier">
                    <field name="supplier_ids" widget="many2many_tags" options="{'no_create': True}" nolabel="True"
                           context="{'search_default_supplier':1, 'default_supplier':1, 'default_customer':0}"
                           domain="[('supplier','=',True)]"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
