#. module: email_send_error_notification
#: model:mail.template,body_html:email_send_error_notification.mail_template_error_notice
msgid ""
"<div>\n"
"                    <p>Hello ${object.author_id.name},</p>\n"
"                    <p>we got a problem with your email:\n"
"                        <i>${object.subject}</i>\n"
"                    </p>\n"
"                    <p>Maybe you used a wrong recipient address?</p>\n"
"                    <p>\n"
"                        <br/>\n"
"                    </p>\n"
"                    <p>Technical details:</p>\n"
"                    <p>\n"
"                        <i>${(object.failure_reason)|safe}</i>\n"
"                    </p>\n"
"                    <p>\n"
"                        <br/>\n"
"                    </p>\n"
"                    <p>Original mail:</p>\n"
"                    <p>\n"
"                        <br/>\n"
"                    </p>\n"
"                    <p>${ctx.get('original_body')}</p>\n"
"                </div>\n"
"            "
msgstr ""
"<div>\n"
"                    <p>Hola ${object.author_id.name},</p>\n"
"                    <p>Hemos encontrado un problema enviando el mail con asunto:\n"
"                        <i>${object.subject}</i>\n"
"                    </p>\n"
"                    <p>¿Puede ser que hayas utilizado una dirección de email incorrecta?</p>\n"
"                    <p>\n"
"                        <br/>\n"
"                    </p>\n"
"                    <p>Detalles tecnicos:</p>\n"
"                    <p>\n"
"                        <i>${(object.failure_reason)|safe}</i>\n"
"                    </p>\n"
"                    <p>\n"
"                        <br/>\n"
"                    </p>\n"
"                    <p>El texto original del mail es:</p>\n"
"                    <p>\n"
"                        <br/>\n"
"                    </p>\n"
"                    <p>${ctx.get('original_body')}</p>\n"
"                </div>\n"
"            "

#. module: email_send_error_notification
#: model:mail.template,subject:email_send_error_notification.mail_template_error_notice
msgid "Mail Send Error with: ${(object.subject)|safe}"
msgstr "Error enviando mail con el asunto: ${(object.subject)|safe}"

